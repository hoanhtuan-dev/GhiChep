@code bash

  sudo vim /etc/default/grub # bỏ ghi chú dòng: GRUB_DISABLE_OS_PROBER=false

  # Chạy lệnh 
  sudo os-prober

  # Chạy lệnh đăng ký config grub
  sudo grub-mkconfig -o /boot/grub/grub.cfg

  reboot

@end
